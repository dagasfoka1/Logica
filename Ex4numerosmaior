#include <stdio.h>
#include <stdlib.h>
int i, maior;
int N[4];

int main()
{
for (i=0;i<4;i++){
    printf("\n Digite um número: \n");
    scanf("%d", & N[i] );
}
maior=N[i];
for (i=0;i<4;i++){
   if(N[i]>maior){
       maior=N[i];
   }
}
printf("O maior número digitado foi: %d", maior);
return 0;
}
